{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-slider-demo-app": {
      "root": "",
      "sourceRoot": "src/demo-app",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": {
              "base": "dist/demo-app"
            },
            "index": "src/demo-app/index.html",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "src/demo-app/tsconfig.app.json",
            "assets": [
              "src/demo-app/favicon.ico",
              "src/demo-app/assets",
              {
                "glob": "**/*",
                "input": "docs/",
                "output": "api-docs/"
              }
            ],
            "styles": [
              "src/demo-app/styles.scss"
            ],
            "scripts": [],
            "browser": "src/demo-app/main.ts"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/demo-app/environments/environment.ts",
                  "with": "src/demo-app/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "ngx-slider-demo-app:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "ngx-slider-demo-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "ngx-slider-demo-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/demo-app/test.ts",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "src/demo-app/tsconfig.spec.json",
            "karmaConfig": "src/demo-app/karma.conf.js",
            "include": [
              "../ngx-slider/**/**.spec.ts",
              "../ngx-slider/**/**.d.ts"
            ],
            "styles": [
              "src/demo-app/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "src/demo-app/favicon.ico",
              "src/demo-app/assets",
              {
                "glob": "**/*",
                "input": "docs/",
                "output": "api-docs/"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false,
    "schematicCollections": [
      "@angular-eslint/schematics"
    ]
  }
}